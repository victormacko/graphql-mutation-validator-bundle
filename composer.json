{
  "name": "victormacko/graphql-mutation-validator-bundle",
  "description": "Provide a base class to validate user data sent in a GraphQL mutation and return validations errors.",
  "type": "symfony-bundle",
  "license": "MIT",
  "keywords": ["GraphQL", "Validation"],
  "authors": [
    {
      "name": "AssoConnect",
      "homepage": "https://www.assoconnect.com"
    },
    {
      "name": "Sylvain Fabre"
    }
  ],
  "autoload": {
    "psr-4": {
      "AssoConnect\\GraphQLMutationValidatorBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "require": {
    "php": "^7 || ^8",
    "overblog/graphql-bundle": "^0.14.1 || ^1.5",
    "symfony/validator": "^5.0 || ^6.0 || ^7.0",
    "doctrine/annotations": "^1.6"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "phpunit/phpunit": "^7 || ^8",
    "symfony/yaml": "^5 || ^6 || ^7",
    "rector/rector": "^1.2"
  }
}
